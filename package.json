{
  "name": "image-downloader",
  "version": "1.0.0",
  "description": "CLI app that allow user to download images from given website URL",
  "main": "src/index.js",
  "type": "module",
  "engines": { "node" : "17.8.0" },
  "scripts": {
    "start": "node ./src/index.js",
    "test": "jest --passWithNoTests",
